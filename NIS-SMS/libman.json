{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "particles.js@2.0.0",
      "destination": "wwwroot/lib/particles.js/",
      "files": [
        "particles.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.3",
      "destination": "wwwroot/jquery-validate/",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "bootswatch@5.1.3",
      "destination": "wwwroot/bootstrap/bootswatch/",
      "files": [
        "cerulean/bootstrap.min.css"
      ]
    }
  ]
}